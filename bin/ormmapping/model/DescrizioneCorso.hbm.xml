<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.DescrizioneCorso" table="DescrizioneCorso" schema="" lazy="false">
		<id name="nome" column="Nome" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<many-to-one name="tipologiaCorso" cascade="lock" column="TipologiaCorsoID" class="model.TipologiaCorso" not-null="true" lazy="no-proxy">
		</many-to-one>
		<property name="prenotazione" column="Prenotazione" type="boolean" length="1" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
